#!/usr/bin/env node
require('source-map-support').install();
var Controller = require('../').lib.Control;

var theController = new Controller();

theController.init('config.js',
  function(err, entities) {

    if (err) {
      console.log(err);
      process.exit(1);
    }
    theController.submitWorkflowExecution('createDeployment',
      { deploymentId: 'test', name: 'Test Name...' }, // initial env/workflow input
      {},                                             // swf workflow exec opts
      (err, result) => {
        console.log(err, result);
      });
  });
